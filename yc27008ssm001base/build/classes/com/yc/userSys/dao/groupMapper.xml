<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yc.userSys.dao.GroupDao">
	<select id="findById" parameterType="int"
		resultType="com.yc.userSys.model.GroupModel">
		select *
		from t_group where id=#{id}
	</select>
	<select id="findAll" resultType="com.yc.userSys.model.GroupModel"> select * from t_group </select>
</mapper>